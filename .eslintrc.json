{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["flowtype"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "comma-dangle": 0,
    "arrow-parens": 0,
    "semi": ["error", "never"],
    "react/jsx-filename-extension": 0,
    "import/no-extraneous-dependencies": 0,
    "no-tabs": 0
  }
}
